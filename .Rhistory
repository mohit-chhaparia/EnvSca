cat_convert <- function(xt){
stage = sort(unique(xt))
nobs = length(xt)
yt = matrix(0,nobs,length(stage)-1)
for (j in 1:length(stage)-1){
yt[,j] = (xt==stage[j])*1
}
return(yt)
}
